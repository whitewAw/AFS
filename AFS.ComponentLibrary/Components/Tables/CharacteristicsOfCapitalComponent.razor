                <TdComponent Align="MR">@Constraints.RoundStr(CharacteristicsOfCapital.TotalAssets.Current.Deviations)</TdComponent>
                <TdComponent Align="MR">@Constraints.RoundStr(CharacteristicsOfCapital.TotalAssets.Current.GrowthRate)</TdComponent>
            </tr>
            <tr>
                <TdComponent Rowspan="2" Align="ML">1.</TdComponent>
                <TdComponent Align="ML">@ResLoc["NonCurrentImmobilizedAssets"]</TdComponent>
                <TdComponent Align="MR">@Constraints.RoundStr(CharacteristicsOfCapital.NonCurrentImmobilizedFunds.Base.BeginningOfyear)</TdComponent>
