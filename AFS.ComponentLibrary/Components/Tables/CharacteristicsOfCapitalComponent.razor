                <TdComponent Align="MR">@Constraints.RoundStr(CharacteristicsOfCapital.NonCurrentImmobilizedFunds.Current.EndOfYear)</TdComponent>
                <TdComponent Align="MR">@Constraints.RoundStr(CharacteristicsOfCapital.NonCurrentImmobilizedFunds.Current.Deviations)</TdComponent>
